{
    "name": "felipemateus/metatags",
    "homepage":"https://github.com/eufelipemateus/metatags" ,
   "keywords":[
       "Metadata",
       "Crawler",
       "Url",
       "Seo",
       "og",
       "twiiter",
       "facebook",
       "google",
       "bing"
     ],
    "description": "A Laravel package to fetch webpage metadata ( Open Graph | Twitter | Facebook | Article ) ect...",
    "type": "library",
    "require": {
        "php": ">=7.4",
        "guzzlehttp/guzzle": "^7.10"
    },
    "license": "MIT",
    "config": {
    	"optimize-autoloader": true
    },
    "authors": [
        {
            "name": "Mobio Solutions",
            "email": "contact@mobiosolutions.com"
        },
        {
            "name": "Felipe Mateus",
            "email": "eu@felipemateus.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "FelipeMateus\\Metatags\\": "src/"
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "extra": {
        "laravel": {
            "providers": [
                "FelipeMateus\\Metatags\\Providers\\MetatagsProvider"
            ],
            "aliases": {
                "Metatags": "FelipeMateus\\Metatags\\Facades\\MetatagsFacade"
            }
        }
    }
}
